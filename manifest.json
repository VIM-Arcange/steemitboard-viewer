{
   "background": {
      "matches": [ "https://steemit.com/*" ],
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "default.png",
      "default_title": "SteemitBoard viewer"
   },
   "description": "View account on SteemitBoard.com",
   "short_name": "Offered by Arcange",
   "manifest_version": 2,
   "name": "SteemitBoard viewer",
   "permissions": [ "http://*/", "https://*/", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "author": "Arcange",
   "homepage_url": "http://steemit.com/@arcange",
   "version": "1.0.8",
   "icons": { "128": "icon-128.png" }
}
